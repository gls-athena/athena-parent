<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- 排除自动生成的代码 -->
    <Match>
        <Class name="~.*\.Q.*"/>
    </Match>

    <!-- 排除测试代码中的某些规则 -->
    <Match>
        <Class name="~.*Test.*"/>
        <Bug pattern="DM_EXIT"/>
    </Match>

    <!-- 排除特定的误报 -->
    <Match>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2"/>
        <Class name="~.*\.dto\..*"/>
    </Match>

    <!-- 排除配置类中的序列化问题 -->
    <Match>
        <Class name="~.*Configuration.*"/>
        <Bug pattern="SE_BAD_FIELD,SE_NO_SERIALVERSIONID"/>
    </Match>

    <!-- 排除Spring Boot自动配置中的未使用字段 -->
    <Match>
        <Class name="~.*Properties.*"/>
        <Bug pattern="UWF_UNWRITTEN_FIELD"/>
    </Match>
</FindBugsFilter>

